<?xml version="1.0" encoding="utf-8"?>
<script>
	<boat> 
		<model_to_y_forwards_rot_1>0 0 1.57079</model_to_y_forwards_rot_1>

		<thrust_force>2000</thrust_force>
		<propellor_point_os>-2.0 0 -0.2</propellor_point_os>
		<propellor_sideways_offset>0.0</propellor_sideways_offset>
		<rudder_deflection_force_factor>0.0</rudder_deflection_force_factor>
		<thrust_vector_lateral_amount>0.3</thrust_vector_lateral_amount>
		
		<jet_particle_initial_width>0.5</jet_particle_initial_width>

		<front_cross_sectional_area>0.1</front_cross_sectional_area>
		<side_cross_sectional_area> 0.4</side_cross_sectional_area>
		<top_cross_sectional_area>  0.4</top_cross_sectional_area>

		<!--right side front -->
		<splash_point>
			<point_os>0.7 -0.6 -0.1</point_os>
			<right_sign>1.0</right_sign>
		</splash_point>

		<!--left side front-->
		<splash_point>
			<point_os>0.7 0.6 -0.1f</point_os>
			<right_sign>-1.0</right_sign>
		</splash_point>

		<!--right side rear-->
		<splash_point>
			<point_os>-1.3 -0.6 -0.1</point_os>
			<right_sign>1.0</right_sign>
		</splash_point>

		<!--left side rear-->
		<splash_point>
			<point_os>-1.3 0.6 -0.1</point_os>
			<right_sign>-1.0</right_sign>
		</splash_point>

		<seat>
			<seat_position>-0.3 0.0 0.85</seat_position>
			<upper_body_rot_angle>-0.5</upper_body_rot_angle>
			<upper_leg_rot_angle>1.5</upper_leg_rot_angle>
			<upper_leg_apart_angle>0.4</upper_leg_apart_angle>
			<lower_leg_rot_angle>-1.4</lower_leg_rot_angle>
			<lower_leg_apart_angle>0.5</lower_leg_apart_angle>

			<arm_down_angle>2.4</arm_down_angle>
			<arm_out_angle>0.05</arm_out_angle>
			<upper_arm_shoulder_lift_angle>0.5</upper_arm_shoulder_lift_angle>
			<lower_arm_up_angle>0.8</lower_arm_up_angle>
		</seat>

		<seat>
			<seat_position>-0.8 0.0 0.85</seat_position>
			<upper_body_rot_angle>-0.5</upper_body_rot_angle>
			<upper_leg_rot_angle>1.5</upper_leg_rot_angle>
			<upper_leg_apart_angle>0.4</upper_leg_apart_angle>
			<lower_leg_rot_angle>-1.4</lower_leg_rot_angle>
			<lower_leg_apart_angle>0.5</lower_leg_apart_angle>

			<arm_down_angle>3.1</arm_down_angle>
			<arm_out_angle>0.05</arm_out_angle>
			<upper_arm_shoulder_lift_angle>0.5</upper_arm_shoulder_lift_angle>
			<lower_arm_up_angle>0.9</lower_arm_up_angle>
		</seat>
	</boat>
</script>
